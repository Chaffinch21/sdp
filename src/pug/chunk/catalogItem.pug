-var Propertys = [{name: 'Гарантия', value:'18 месяцев'}, {name: 'Артикул', value:'AAA41269004'}, {name: 'Длина', value:'163 см'}, {name: 'Высота', value:'90 см'}, {name: 'Глубина', value:'108 см'}, {name: 'Спальное место', value:'AAA41269004'}, {name: 'Артикул', value:'200 см х 125 см'}, {name: 'Высота посадки', value:'45 см'}, {name: 'Механизм', value:'Опорный'}, {name: 'Тип обивки', value:'Рогожка'}, {name: 'Цвет', value:'Синий'}, {name: 'Наполнитель', value:'Независимый пружинный блок'}, {name: 'Материал каркаса', value:'Металл. рама с дерев. латами'}, {name: 'Количество мест', value:'Двухместные'}, {name: 'Категория', value:'Прямые тканевые диваны'}, {name: 'Возврат', value:'Условия'}]
section.production
  .container.production__container
    .production__wrap
      include thumbslider
      .production__description
        h3.production__subtitle Опорный механизм
        p.production__text Опорный механизм напоминает пантограф, к которому добавили дополнительную секцию. У опорного дивана сиденье «выпрыгивает» вперёд и вытаскивает вторую часть спального места. Заключительный штрих — опустить спинку.
        p.production__text Этот механизм не портит пол и не боится ковров. Требуется чуть больше силы, чтобы разложить диван, но подросток справится.
        .production__draws
          img.production__draw(src="img/production/draw1.png")
          img.production__draw(src="img/production/draw2.png")
      .primery.production__primery
        .card__reiting.primery__reiting.production__reiting 4.8
        h2.title.primery__title.production__title Диван тканевый прямой “D-31”
        p.primery__descr.production__descr Мини 120 см без ящика Stark синий (Рогожка)
        p.primery__price.production__price 28 490 руб
        button.primery__sale.production__sale Купить в один клик
        button.primery__cart.production__cart + Добавить в корзину
      dl.propertys.production__propertys
        each item in Propertys
          .propertys__inner
            dt.propertys__key
              span.propertys__name= item.name
            dd.propertys__value= item.value